            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="70.0" text="ป้ายที่จอด">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="102.0" prefHeight="40.0" prefWidth="89.0" text="ชื่อป้าย :    ">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="92.0" layoutY="110.0" prefHeight="25.0" prefWidth="189.0" />
      <Label layoutX="14.0" layoutY="160.0" prefHeight="40.0" prefWidth="110.0" text="ชื่ออื่น (ถ้ามี) :">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TextField fx:id="otherNameField" layoutX="130.0" layoutY="168.0" prefHeight="25.0" prefWidth="151.0" />
